{
    "breadcrumbs.enabled": false,
    "debug.console.fontSize": 12,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.openDebug": "openOnDebugBreak",
    "debug.toolBarLocation": "docked",
    "editor.autoIndent": "none",
    "editor.bracketPairColorization.enabled": false,
    "editor.colorDecorators": false,
    "editor.fontFamily": "Iosevka NFM",
    "editor.fontSize": 12,
    "editor.formatOnPaste": false,
    "editor.hover.enabled": true,
    "editor.inlayHints.fontSize": 10,
    "editor.letterSpacing": 0.3,
    "editor.lightbulb.enabled": false,
    "editor.lineHeight": 19,
    "editor.minimap.enabled": false,
    "editor.renderFinalNewline": "off",
    "editor.renderWhitespace": "trailing",
    "editor.showUnused": false,
    "editor.stickyScroll.enabled": false,
    "explorer.compactFolders": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "html.format.enable": false,
    "javascript.suggestionActions.enabled": false,
    "keyboard.dispatch": "keyCode",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.opacity": 0.7,
    "material-icon-theme.saturation": 0.7,
    "security.workspace.trust.untrustedFiles": "open",
    "tailwind-fold.autoFold": false,
    "telemetry.telemetryLevel": "off",
    "terminal.external.linuxExec": "alacritty",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.lineHeight": 1,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.whichwrap": "<,>,[,]",
    "vscode_custom_css.imports": ["file:///home/kh3phr3n/.vscode-oss/styles.css"],
    "vscode_custom_css.policy": true,
    "window.customTitleBarVisibility": "never",
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "window.zoomLevel": -0.6,
    "workbench.activityBar.location": "hidden",
    "workbench.colorTheme": "Carbonfox",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",

    "[go]": {
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {
            "source.organizeImports": false
        }
    },
    "gopls": {
        "ui.semanticTokens": true,
        "completeUnimported": false
    },

    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },

    "emmet.includeLanguages": {
        "edge": "html"
    },

    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*": {
                "fontStyle": ""
            },
            "*.mutable": {
                "underline": false
            }
        }
    },

    "workbench.colorCustomizations": {
        "editorInlayHint.background": "#000000",
        "editorInlayHint.foreground": "#FFFFFF"
    }
}

