{
    "breadcrumbs.enabled": false,
    "debug.console.fontSize": 13,
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.openDebug": "openOnDebugBreak",
    "debug.toolBarLocation": "docked",
    "editor.autoIndent": "none",
    "editor.bracketPairColorization.enabled": false,
    "editor.colorDecorators": false,
    "editor.fontFamily": "Iosevka Term Semibold",
    "editor.fontSize": 13,
    "editor.formatOnPaste": false,
    "editor.hover.enabled": true,
    "editor.inlayHints.fontSize": 10,
    "editor.letterSpacing": 0.5,
    "editor.lightbulb.enabled": false,
    "editor.lineHeight": 17,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "trailing",
    "editor.showUnused": false,
    "explorer.compactFolders": false,
    "explorer.openEditors.visible": 0,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "html.format.enable": false,
    "javascript.suggestionActions.enabled": false,
    "keyboard.dispatch": "keyCode",
    "material-icon-theme.hidesExplorerArrows": true,
    "material-icon-theme.opacity": 0.7,
    "material-icon-theme.saturation": 0.7,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "terminal.external.linuxExec": "urxvt",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.fontWeightBold": "normal",
    "terminal.integrated.letterSpacing": 1,
    "terminal.integrated.lineHeight": 1,
    "vim.useCtrlKeys": false,
    "vim.useSystemClipboard": true,
    "vim.visualstar": true,
    "vim.whichwrap": "<,>,[,]",
    "vscode_custom_css.imports": ["file:///home/kh3phr3n/.vscode-oss/styles.css"],
    "vscode_custom_css.policy": true,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "One Dark Pro Flat",
    "workbench.editor.untitled.hint": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",

    "[markdown]": {
        "files.trimTrailingWhitespace": false
    },

    "emmet.includeLanguages": {
        "edge": "html"
    },

    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*": {
                "fontStyle": ""
            },
            "*.mutable": {
                "underline": false
            }
        }
    },

    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#8D95A7",
        "editorInlayHint.background": "#282C34",
        "editorInlayHint.foreground": "#999999",
        "focusBorder": "#21252B",
        "tab.activeBorder": "#00000000",
        "terminalCursor.foreground": "#8D95A7"
    }
}

